{
    "name": "qzion/acl",
    "description": "A set of access control tools for Laravel",
    "require": {
        "php" : ">=7.0",
        "illuminate/auth": "~5.3.0|~5.4.0|~5.5.0",
        "illuminate/container": "~5.3.0|~5.4.0|~5.5.0",
        "illuminate/contracts": "~5.3.0|~5.4.0|~5.5.0",
        "illuminate/database": "~5.4.0|~5.5.0"
    },
    "require-dev": {
        "monolog/monolog": "^1.22",
        "orchestra/testbench": "~3.3.0|~3.4.2|~3.5.0",
        "phpunit/phpunit": "^5.7|6.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Quinones",
            "email": "CQuinones@Qzion.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Qzion\\Acl\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Qzion\\Acl\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
